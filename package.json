{
  "name": "color-shift",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "description": "A client-side web application for designers and developers to convert colors between different CSS color formats and batch-process CSS/SASS/PostCSS files.",
  "author": "Mohamed Fadl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FADL285/ColorShift"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{js,ts,vue,css,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nuxt/eslint": "^1.11.0",
    "@nuxt/hints": "^1.0.0-alpha.3",
    "@nuxt/test-utils": "^3.21.0",
    "@nuxt/ui": "^4.2.1",
    "culori": "^4.0.2",
    "eslint": "^9.39.1",
    "nuxt": "^4.2.1",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.79",
    "@types/culori": "^4.0.1",
    "@vueuse/nuxt": "^14.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
